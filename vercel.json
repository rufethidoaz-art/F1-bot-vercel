{
    "version": 2,
    "name": "f1-bot-vercel",
    "functions": {
        "app.py": {
            "runtime": "@vercel/python",
            "maxDuration": 300
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "app.py"
        },
        {
            "src": "/health",
            "dest": "app.py"
        },
        {
            "src": "/webhook",
            "dest": "app.py"
        }
    ]
}
